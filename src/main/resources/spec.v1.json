          "type": "string",
          "minLength": 1
        },
        "label": {
          "type": "string",
          "minLength": 1
        },
        "properties": {
          "type": "array",
          "items": {
            "type": "string",
            "minLength": 1
          },
          "minItems": 1
        },
        "options": {
          "type": "object"
        }
      },
      "required": [
        "name",
        "label",
        "properties"
      ],
      "additionalProperties": false
    },
    "target.node.schema.constraints.unique": {
